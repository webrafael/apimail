{
    "name": "rafaelandrade/mail",
    "type": "library",
    "description": "Api desenvolvida para envio de emails",
    "version":"1.0",
    "config": {
        "platform": {
            "php": "7.0"
        }
    },
    "require": {
        "php": ">=7.0",
        "ext-ctype": "*",
        "ext-filter": "*"
    },

    "require-dev": {
        "phpmailer/phpmailer": "dev-master",
        "symfony/dotenv": "^3.3"
    },
    "authors": [
        {
            "name": "Rafael Andrade",
            "email": "web.andrade@hotmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mail\\": "src/WebRafael/Mail"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mail\\Test\\": "src/test"
        }
    },
    "minimum-stability":"dev-master",
    "license": "LGPL-2.1"
}
